{
  "swagger" : "2.0",
  "info" : {
    "version" : "1",
    "title" : "Aiham Swaggerhub Test 1",
    "description" : "Testing swaggerhub",
    "contact" : {
      "name" : "Aiham Hammami",
      "url" : "http://www.aiham.net",
      "email" : "aiham@aiham.net"
    },
    "license" : {
      "name" : "MIT"
    }
  },
  "definitions" : {
    "Foo" : {
      "type" : "object",
      "properties" : {
        "id" : {
          "type" : "integer",
          "format" : "int64"
        },
        "name" : {
          "type" : "string"
        },
        "email" : {
          "type" : "string",
          "format" : "email"
        },
        "dob" : {
          "type" : "string",
          "format" : "date"
        },
        "website" : {
          "type" : "string",
          "format" : "url"
        }
      }
    }
  },
  "paths" : {
    "/foo" : {
      "get" : {
        "summary" : "Get Foos",
        "responses" : {
          "200" : {
            "description" : "Returns list of Foos",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Foo"
              }
            }
          }
        }
      },
      "post" : {
        "summary" : "Create a Foo",
        "parameters" : [ {
          "name" : "foo",
          "in" : "body",
          "schema" : {
            "$ref" : "#/definitions/Foo"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Returns created Foo",
            "schema" : {
              "type" : "object",
              "properties" : {
                "name" : {
                  "type" : "string"
                }
              }
            }
          }
        }
      }
    },
    "/foo/{id}" : {
      "get" : {
        "summary" : "Get a Foo",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "type" : "integer",
          "required" : true
        } ],
        "responses" : {
          "200" : {
            "description" : "Returns requested Foo",
            "schema" : {
              "$ref" : "#/definitions/Foo"
            }
          }
        }
      },
      "put" : {
        "summary" : "Update a Foo",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "type" : "integer",
          "required" : true
        }, {
          "name" : "foo",
          "in" : "body",
          "schema" : {
            "$ref" : "#/definitions/Foo"
          }
        } ],
        "responses" : {
          "200" : {
            "description" : "Returns updated Foo",
            "schema" : {
              "$ref" : "#/definitions/Foo"
            }
          }
        }
      },
      "delete" : {
        "summary" : "Delete a Foo",
        "parameters" : [ {
          "name" : "id",
          "in" : "path",
          "type" : "integer",
          "required" : true
        } ],
        "responses" : {
          "200" : {
            "description" : "No response for successful delete"
          }
        }
      }
    }
  }
}